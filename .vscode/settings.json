{
    "cmake.configureOnOpen": false,
    "cmake.configureOnEdit": false,
    "cmake.buildDirectory": "${workspaceFolder}/build",
    "cmake.sourceDirectory": "${workspaceFolder}/cetlvast",
    "cmake.buildBeforeRun": true,
    "cmake.configureSettings": {
        "FETCHCONTENT_UPDATES_DISCONNECTED": true,
        "FETCHCONTENT_FULLY_DISCONNECTED": true,
        "CMAKE_MESSAGE_LOG_LEVEL": "STATUS",
        "CYPHAL_PROJECT_VERSION": "0.0.0"
    },
    "cmake.buildArgs": [
        "--verbose"
    ],
    "cmake.configureArgs": [
        "--warn-uninitialized"
    ],
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "editor.wordWrapColumn": 120,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "python.linting.mypyEnabled": true,
    "python.linting.enabled": true,
    "testMate.cpp.test.advancedExecutables": [
        {
            "pattern": "${workspaceFolder}/build/suites/unittest/test_*"
        },
        {
            "pattern": "${workspaceFolder}/build/suites/docs/examples/example_*"
        }
    ],
    "sonarlint.pathToCompileCommands": "${workspaceFolder}/build/compile_commands.json",
    "sonarlint.connectedMode.project": {
        "connectionId": "opencyphal",
        "projectKey": "OpenCyphal_CETL"
    },
    "sonarlint.analyzerProperties": {
        "sonar.cfamily.reportingCppStandardOverride": "c++14"
    },
    "cSpell.words": [
        "cetl",
        "DCETL_",
        "cetlvast",
        "DCETLVAST_",
