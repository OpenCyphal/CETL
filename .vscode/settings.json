{
    "cmake.configureOnOpen": false,
    "cmake.configureOnEdit": false,
    "cmake.buildDirectory": "${workspaceFolder}/cetlvast/build_vscode/build",
    "cmake.sourceDirectory": "${workspaceFolder}/cetlvast",
    "cmake.buildBeforeRun": true,
    "cmake.configureArgs": [
        "-DCETLVAST_EXT_FOLDER=${workspaceFolder}/cetlvast/build_vscode/ext",
        "--warn-uninitialized"
    ],
    "editor.wordWrapColumn": 120,
    "editor.rulers": [
        120
    ],
    "files.eol": "\n",
    "files.insertFinalNewline": true,
